generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Configuration {
  id        String   @id @default(uuid())
  key       String
  value     String
  label     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("configurations")
}
